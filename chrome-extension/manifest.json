{
  "manifest_version": 3,
  "name": "Clawatar Meeting Audio",
  "version": "1.2",
  "description": "Captures meeting tab audio for Clawatar VRM avatar",
  "permissions": ["tabCapture"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}
